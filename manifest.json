{
  "manifest_version": 2,
  "name": "Fast tab",
  "version": "1.0",
  "description": "Creates a centered popup listing open tabs.",
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Fast tab"
  },
  "commands": {
    "open-popup": {
      "suggested_key": { "default": "Ctrl+Alt+S" },
      "description": "Send a 'toggle-feature' event to the extension"
    }
  },
  "web_accessible_resources": [
    "popup.html",
    "popup.css"
  ]
}
